{
    "manifest_version": 3,
    "name": "Site Blocker",
    "version": "1.0",
    "description": "Blocks access to Graphic websites.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "incognito": "split",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  