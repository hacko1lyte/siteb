{
    "manifest_version": 2,
    "name": "Site Blocker",
    "version": "1.0",
    "description": "Blocks access to Graphic websites.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "incognito": "split",
    "icons": {
      "16": "icons/download.jpeg"
     
    }
  }
  