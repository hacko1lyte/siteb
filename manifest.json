{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Blocks websites listed in an external blocklist.",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "storage"
  ],
  "host_permissions": [
    "https://hacko1lyte.github.io/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}